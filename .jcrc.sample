JC_SAMPLE_INPUT="
diff --git a/src/server.ts b/src/server.ts
index ad4db42..f3b18a9 100644
--- a/src/server.ts
+++ b/src/server.ts
@@ -10,7 +10,7 @@
+    const PORT = 7799;
@@ -34,6 +34,6 @@
