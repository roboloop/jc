JC_API_KEY=
JC_MODEL=gpt-5-2025-08-07
JC_GIT_DIFF_ARGS="
:(exclude)*.lock
:(exclude)*-lock.*
"
JC_MAX_INPUT_TOKENS=8192
JC_MAX_OUTPUT_TOKENS=2048
JC_LOG_FILE=

# Do not forget to escape backslashes, quotes, backticks and dollar signs
JC_INSTRUCTIONS="You write Git commit messages from diffs."

# Do not forget to escape backslashes, quotes, backticks and dollar signs
